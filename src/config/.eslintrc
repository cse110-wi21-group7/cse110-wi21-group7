{
    "plugins": ["prettier"],
    
    "rules": {
      "prettier/prettier": "error"
    },
    
    "extends": [
      "plugin:prettier/recommended"
    ],
    
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },

    "env": {
      "browser": true,
      "es2021": true
    },

    "ignorePatterns": ["docs/", "node_modules/"]
}


